package problem6;

import static org.junit.Assert.*;

import org.junit.Test;

public class Problem6Test {

	@Test
	public void outputTesterPath1() {
		Problem6 obj = new Problem6();
		obj.n = 2;
		int arr[] = {2,5};
		obj.a = arr;
		obj.init();
		int actual = obj.res;
		assertEquals(2, actual);
	}
	
	@Test
	public void outputTesterPath2() {
		Problem6 obj = new Problem6();
		obj.n = 2;
		int arr[] = {5, 2};
		obj.a = arr;
		obj.init();
		int actual = obj.res;
		assertEquals(2, actual);
	}

}
