package problem1;

import static org.junit.Assert.*;

import org.junit.Test;

public class Problem1Test {

	@Test
	public void bigmodOutputTester() {
		
		Problem1 obj = new Problem1();
		
		int res1 = obj.bigmod(1, 0);
		assertEquals(1, res1);
		
		int res2 = obj.bigmod(2, 1);
		assertEquals(2, res2);
		
		int res3 = obj.bigmod(2, 2);
		assertEquals(4, res3);
	}

}
